<?xml version="1.0"?>
<gconfschemafile>
<schemalist>
	<schema>
	  <key>/schemas/apps/gnome_settings_daemon/gtk-modules/globalmenu-gnome</key>
	  <applyto>/apps/gnome_settings_daemon/gtk-modules/globalmenu-gnome</applyto>
	  <owner>libglobalmenu-gnome</owner>
	  <type>bool</type>
	  <default>false</default>
	  <locale name="C">
		<short>Enable Global Menu For GTK applications</short>
		<long>If this key is true, libglobalmenu-gnome, the GTK plugin for Global Menu is loaded.</long>
	  </locale>
	</schema>
	<schema>
		<key>/schemas/apps/gnome-applet-globalmenu/prefs/disable_module_check</key>
		<owner>gnome-applet-globalmenu</owner>
		<type>bool</type>
		<default>FALSE</default>
		<locale name="C">
			<short>Disable checking the GTK plugin module at startup</short>
			<long>Disable checking the GTK plugin module at startup. If you know the module is loaded but for some reasons the applet cannot correctly detect this fact, set this key to true to skip the checking.</long>
		</locale>
	</schema>
	<schema>
		<key>/schemas/apps/gnome-applet-globalmenu/prefs/has_handle</key>
		<owner>gnome-applet-globalmenu</owner>
		<type>bool</type>
		<default>FALSE</default>
		<locale name="C">
			<short>Add a handle to the applet</short>
			<long>Specify if the applet has a handle. When the handle is enabled, the applet will not expand to occupy the remaining empty area on the panel.</long>
		</locale>
	</schema>
	<schema>
		<key>/schemas/apps/gnome-applet-globalmenu/prefs/show_icon</key>
		<owner>gnome-applet-globalmenu</owner>
		<type>bool</type>
		<default>FALSE</default>
		<locale name="C">
			<short>Show the icon of the action window in the applet</short>
			<long>Show the icon of the active window in the applet when enabled.</long>
		</locale>
	</schema>
	<schema>
		<key>/schemas/apps/gnome-applet-globalmenu/prefs/show_name</key>
		<owner>gnome-applet-globalmenu</owner>
		<type>bool</type>
		<default>TRUE</default>
		<locale name="C">
			<short>Show the name of the active application in the applet</short>
			<long>Show the name of the active application in the applet when enabled</long>
		</locale>
	</schema>
	<schema>
		<key>/schemas/apps/gnome-applet-globalmenu/prefs/show_window_list</key>
		<owner>gnome-applet-globalmenu</owner>
		<type>bool</type>
		<default>TRUE</default>
		<locale name="C">
			<short>Enabled the Window List</short>
			<long>Switch among applications via the application name label</long>
		</locale>
	</schema>
	<schema>
		<key>/schemas/apps/gnome-applet-globalmenu/prefs/show_window_actions</key>
		<owner>gnome-applet-globalmenu</owner>
		<type>bool</type>
		<default>TRUE</default>
		<locale name="C">
			<short>Enable the Window Actions</short>
			<long>Add window action menu entries to active application name menu</long>
		</locale>
	</schema>
	<schema>
		<key>/schemas/apps/gnome-applet-globalmenu/prefs/title_max_width</key>
		<owner>gnome-applet-globalmenu</owner>
		<type>int</type>
		<default>-1</default>
		<locale name="C">
			<short>Maximum length of the title area</short>
			<long>The maximum length of name of the active application, in pixels</long>
		</locale>
	</schema>
	<schema>
		<key>/schemas/apps/gnome-applet-globalmenu/prefs/use_rgba_colormap</key>
		<owner>gnome-applet-globalmenu</owner>
		<type>bool</type>
		<default>FALSE</default>
		<locale name="C">
			<short>Enable the RGBA support(experimental)</short>
			<long>Enable the RGBA support for submenus. Restart the applet to take effects.</long>
		</locale>
	</schema>
	<schema>
		<key>/schemas/apps/gnome-applet-globalmenu/prefs/use_rgba_colormap</key>
		<owner>gnome-applet-globalmenu</owner>
		<type>bool</type>
		<default>FALSE</default>
		<locale name="C">
			<short>Enable the RGBA support(experimental)</short>
			<long>Enable the RGBA support for submenus. Restart the applet to take effects.</long>
		</locale>
	</schema>
	<schema>
		<key>/schemas/apps/gnome-applet-globalmenu/prefs/enable_search_box</key>
		<owner>gnome-applet-globalmenu</owner>
		<type>bool</type>
		<default>FALSE</default>
		<locale name="C">
			<short>Add a search box in the drop down menu</short>
			<long>Add a search box in the dropdown menu, where you can find entries by typing the name.</long>
		</locale>
	</schema>
	<schema>
		<key>/schemas/apps/gnome-applet-globalmenu/prefs/grab_mnemonic_keys</key>
		<owner>gnome-applet-globalmenu</owner>
		<type>bool</type>
		<default>TRUE</default>
		<locale name="C">
			<short>Allow accessing the menu items by the mnemonic keys</short>
			<long>Allow accessing the menu items by the mnemonic keys. When this option is disabled, the topmost menu items are not directly accessible from keyboard.</long>
		</locale>
	</schema>
</schemalist>
</gconfschemafile>
