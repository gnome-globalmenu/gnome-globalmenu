msgid "On-line help"
msgstr "在线帮助"

#: ../applet/applet.vala:324
msgid "Global Menu Applet Preferences"
msgstr "全局菜单首选项"

#: ../applet/switcher.vala:68
msgid "no windows"
msgstr "没有窗口"

#: ../applet/switcher.vala:69
msgid "no actions"
msgstr "无可用操作"

